<p style="color: green"><%= notice %></p>

<%= render @beerclub %>

<h3>members</h3>
<% @beerclub.members.each do |member| %>
  <%= link_to "#{member.username}", member %>
<% end %>

<div>
  <%= link_to "Edit this beerclub", edit_beerclub_path(@beerclub) %> |
  <%= link_to "Back to beerclubs", beerclubs_path %>

  <%= button_to "Destroy this beerclub", @beerclub, method: :delete %>
</div>
